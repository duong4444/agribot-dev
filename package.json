{
  "name": "agri-chatbot",
  "version": "1.0.0",
  "description": "Agricultural Chatbot System with AI, IoT, and Farm Management",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "db:migrate": "turbo db:migrate",
    "db:seed": "turbo db:seed",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "setup": "pnpm install && pnpm db:migrate && pnpm db:seed"
  },
  "keywords": ["agriculture", "chatbot", "iot", "ai", "nextjs", "nestjs"],
  "author": "Your Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.7",
    "prettier": "^3.4.2",
    "turbo": "^2.5.6",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
